summarise("Unique start station latitudes" = n_distinct(start_lat), "Unique start station longitudes" = n_distinct(start_lng), "Unique end station latitudes" = n_distinct(end_lat), "Unique end station longitudes" = n_distinct(end_lng)) %>%
